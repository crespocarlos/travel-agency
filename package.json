{
  "name": "travel-agency",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build:client": "yarn workspace @travel-agency/client build",
    "build:server": "yarn workspace @travel-agency/server build",
    "start:client": "yarn workspace @travel-agency/client start",
    "start:server": "yarn workspace @travel-agency/server start",
    "start:dev:server": "yarn workspace @travel-agency/server start:dev",
